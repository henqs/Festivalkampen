# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

# TEST JS ROUTES
GET    /assets/javascripts/routes   controllers.QuizController.javascriptRoutes()

# skicka quiz-svar
GET   /postquiz                     controllers.QuizController.sayHello(data: String)
GET    /quiz                        controllers.Application.quiz()    



# Testgrejer under h√§r

GET     /showOK                     controllers.Answer.list()    



# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
